{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/web/static/$1"
    },
    {
      "src": "/web/?",
      "dest": "/web/static/index.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/web/static/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "DEBUG": "false",
    "SERVERLESS": "true"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 10
    }
  }
}
